<script src="~/staticfiles/shared/sweetalert2/sweetalert2.all.min.js"></script>
@if (!string.IsNullOrWhiteSpace((string)TempData["Message"]) && (bool)TempData["IsSucceeded"])
{
    string Message = (string)TempData["Message"];
    <script>
        toastr.success('@Message')
    </script>
}
@if (!string.IsNullOrWhiteSpace((string)TempData["Message"]) && !(bool)TempData["IsSucceeded"])
{
    string Message = (string)TempData["Message"];
    <script>
        toastr.error('@Message')
    </script>
}